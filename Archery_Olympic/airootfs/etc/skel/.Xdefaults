URxvt.intensityStyles:		false
URxvt.background:		[85]#000000
URxvt.depth:			32
URxvt.font:			xft:Inconsolata:size=16
URxvt.scrollBar:		false
URxvt.cursorColor:		white
URxvt.perl-lib:			$HOME/.urxvt/ext
URxvt.perl-ext-common:		default,matcher,resize-font,url-select
URxvt.matcher.button:		1
URxvt.resize-font.smaller:	C-j
URxvt.resize-font.bigger:	C-k
URxvt.urlLauncher:		firefox
URxvt.underlineURLs:		true
URxvt.keysym.M-u:		perl:url-select:select_next
URxvt.iso14755:			false
URxvt.iso14755_52:		false
URxvt.colorUL:			#3465a4

rofi.color-enabled:		true
rofi.color-window:		#000, #000, #000
rofi.color-normal:		#111, #819396, #222, #6B1B00, #ffffff
rofi.color-active:		#002b37, #6B1B00, #003643, #6B1B00, #66c6ff
rofi.color-urgent:		#002b37, #da4281, #003643, #6B1B00, #890661

rofi.fake-transparency:		true
rofi.lines:			5
rofi.bw:			0
rofi.opacity:			"10"
rofi.hide-scrollbar:		true
rofi.width:			30
